{
	"tabWidth": 4,
	"useTabs": true,
	"bracketSpacing": true,
	"singleQuote": false,
	"endOfLine": "crlf",
	"trailingComma": "all",
	"semi": false,
	"plugins": [
		"@trivago/prettier-plugin-sort-imports"
	],
	"importOrder": [
		"^react$",
		"<THIRD_PARTY_MODULES>",
		"^@/types",
		"^@/store",
		"^@/store/(.*)$",
		"^@/features/(.*)$",
		"^@/utils/(.*)$",
		"^@/components/(.*)$",
		"^@/layout/(.*)$",
		"^@/pages/(.*)$",
		"^[./]"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}